set history=500
filetype plugin on
filetype indent on
syntax enable
set ignorecase
set smartcase
set incsearch
set hlsearch
set showmatch
set noerrorbells
set novisualbell
set encoding=utf8
set ffs=unix
set nobackup
set nowb
set noswapfile
set showmode
set showcmd
set lazyredraw
set mouse=v
set relativenumber
set ttyfast
set undolevels=999
set title
set number
set colorcolumn=80
